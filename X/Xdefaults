! medium
XTerm*faceSize4: 21
! large
XTerm*faceSize5: 24
! huge
XTerm*faceSize6: 27

! dont include the newline when triple clicking
xterm*cutNewLine: true

! dumb fix for getting the copy into clipboard as well as primary
! you can still use mouse paste in the xterm but paste ctrl-v or whatever
! will work in other apps
! I would rather have a keyboard shortcut to get things into the clipboard, 
! but I dont know how
xterm*VT100.translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)


xterm*saveLines: 2000
xterm*scrollBar: false
xterm*scrollKey: true
xterm*scrollTtyOutput: false
! these go as "charnum1":48,"charnum2":48,"char-range":48,...
xterm*charClass: 43:48,45-47:48,58:48,126:48
! xterm*cursorColor: yellow
xterm*rightScrollBar: true
! just show selected parts of line
xterm*highlightSelection: true
xterm*utf8:    2

! support sixel graphics
xterm*decTerminalID: vt340

