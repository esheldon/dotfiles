customization: -color

! This means never use overstriking for bold
!xterm*boldMode: false
xterm*allowBoldFonts: false


! This means just display a different color, dont use the bold font
!xterm*colorBDMode: true

XTerm*utf8: 1

! zevv-peep-iso8859-1-08x16.bdf
