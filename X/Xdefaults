! xterm*font: -jmk-Neep Alt-Medium-R-Normal--24-240-75-75-C-120-ISO8859-2

! to override with fixed font on command line use
! xterm -fn fixed -xrm 'XTerm*renderFont: false'
! XTerm*faceName: Inconsolata
! XTerm*faceName: Terminus

! default
